<tbody id="tb_model">
        <!-- vòng lặpA -->
        <!-- STT -->
        <% var stt = 1 %>
      <% if(ds_taikhoan.length)
      {ds_taikhoan.forEach(function(e, i){%>
        <tr>
          <td class="text-center"><%= stt++ %></td>
          <% if(locals.Role == 1){%>
            <td class="text-center"><%= e.username %></td>
           <% }else{%> 
             <td class="text-center"><%- locals.ahi_user %></td>
           <%} %>               
          <td class="text-center">
            <% if(locals.Role == 1){%>
            <button class="btn btn-info btn-sm change_password" style="color: #742bbd;" idtk = "<%= e.stt %>" title="Thay đổi mật khẩu"><i class="fa fa-lock"></i></button>
            <button class="btn btn-primary btn-sm edit" idtk = "<%= e.stt %>" title="Cập nhật thông tin tài khoản"><i class="fa fa-pencil"></i></button>
            <button class="btn btn-danger btn-sm delete" idtk = "<%= e.stt %>" title="Xóa thông tin tài khoản"><i class="fa fa-trash-o "></i></button>  
            <% }else{%>
            <button class="btn btn-info btn-sm" disabled style="color: #742bbd;" title="Không có quyền này"><i class="fa fa-lock"></i></button>
            <button class="btn btn-primary btn-sm" disabled title="Không có quyền này"><i class="fa fa-pencil"></i></button>
            <button class="btn btn-danger btn-sm" disabled title="Không có quyền này"><i class="fa fa-trash-o "></i></button>  
            <%} %>        
          </td>
        </tr>
      <% })}else{%>
        <tr class="text-center text-danger">
          <td colspan="9">
          Không có dữ liệu
        </td>
        </tr>
        <%} %>
    <!-- kết thúc vòng lặp -->
    </tbody>