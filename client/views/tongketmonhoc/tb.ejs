<tbody id="tb_model">
  <!-- vòng lặpA -->
  <!-- STT -->
  <% if(ds_tongketmonhoc.length)
      {ds_tongketmonhoc.forEach(function(e, i){%>
  <tr>
    <td><%= e.ma_hs %></td>
    <td><%= e.ten_hs %></td>
    <td><%= e.ten_mh %></td>
    <td><%= e.diem_tkmh %></td>
    <td><%= e.namhoc %></td>
    <td class="text-center">
      <% if(locals.Role == 1){%>
      <button class="btn btn-danger btn-xs delete" idtkmh="<%= e.mamhtk %>" title="Xóa tổng kết môn học"><i class="fa fa-trash-o "></i></button>
      <% }else{%>
      <button class="btn btn-danger btn-xs" disabled title="Không có quyền này"><i class="fa fa-trash-o "></i></button>
      <%} %>
    </td>
  </tr>
  <% })}else{%>
  <tr class="text-center text-danger">
    <td colspan="9">
      Không có dữ liệu
    </td>
  </tr>
  <%} %>
  <!-- kết thúc vòng lặp -->
</tbody>