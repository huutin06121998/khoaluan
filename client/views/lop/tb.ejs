<tbody id="tb_model">
        <!-- vòng lặpA -->
        <!-- STT -->
      <% if(ds_lop.length)
      {ds_lop.forEach(function(e, i){%>
        <tr>
          <td><%= e.ma_lop %></td>
          <td><%= e.ten_lop %></td>
          <td>
              <%= e.ten_gv %>
          </td>          
          <td class="text-center">
            <% if(locals.Role == 1){%>
              <button class="btn btn-primary btn-xs edit" id_lop = "<%= e.stt %>" title="Cập nhật lớp học"><i class="fa fa-pencil"></i></button>
              <button class="btn btn-danger btn-xs delete" id_lop = "<%= e.stt %>" title="Xóa thông tin lớp học"><i class="fa fa-trash-o "></i></button>    
              <% }else{%>
              <button class="btn btn-primary btn-xs" disabled title="Không có quyền này"><i class="fa fa-pencil"></i></button>
              <button class="btn btn-danger btn-xs" disabled title="Không có quyền này"><i class="fa fa-trash-o "></i></button>  
              <%} %>
          </td>
        </tr>
      <% })}else{%>
        <tr class="text-center text-danger">
          <td colspan="9">
          Không có dữ liệu
        </td>
        </tr>
        <%} %>
    <!-- kết thúc vòng lặp -->
    </tbody>